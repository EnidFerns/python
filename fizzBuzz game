#fizzbuzz
n=int(input("entetr the number of numbers you want: "))
f=int(input("input the number that takes fizz: "))
b=int(input("input the number that takes buzz: "))
#nos that take both say fizzbuzz
for i in range(1,n+1):
    if(i%f==0 and i%b==0):
        print(i," = fizzbuzz") 
    else:
        if(i%f==0):
            print(i," = fizz")
        else:
            if(i%b==0):
                print(i," = buzz")
            else:
                print(i)
